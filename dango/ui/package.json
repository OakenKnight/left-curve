{
  "name": "dango-ui",
  "version": "0.1.0",
  "author": "left-curve software",
  "scripts": {
    "dev": "turbo dev",
    "dev:portal": "turbo dev -F @dango/portal -F @dango/shared",
    "lint": "turbo lint",
    "lerna": "lerna",
    "clean": "lerna clean -y && pnpm -r exec rm -rf build && pnpm -r exec rm -rf .turbo",
    "storybook": "pnpm shared sb:dev",
    "shared": "pnpm -F shared",
    "landing": "pnpm -F landing",
    "portal": "pnpm -F portal"
  },
  "engines": {
    "pnpm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "lerna": "^8.1.8",
    "tsup": "^8.3.0",
    "turbo": "^2.0.11",
    "typedoc": "^0.26.5",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "typescript",
        "tsup",
        "@biomejs/biome"
      ]
    }
  }
}
